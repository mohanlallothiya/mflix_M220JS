#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/mflix-js/node_modules/.pnpm/lint-staged@7.3.0/node_modules/lint-staged/index.js/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/lint-staged@7.3.0/node_modules/lint-staged/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/lint-staged@7.3.0/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/node_modules:/home/workspace/mflix-js/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/mflix-js/node_modules/lint-staged/index.js/node_modules:/home/workspace/mflix-js/node_modules/lint-staged/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../lint-staged/index.js" "$@"
else 
  exec node  "$basedir/../lint-staged/index.js" "$@"
fi
