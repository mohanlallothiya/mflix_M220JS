#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/mflix-js/node_modules/.pnpm/eslint-config-prettier@3.6.0_eslint@4.19.1/node_modules/eslint-config-prettier/bin/cli.js/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/eslint-config-prettier@3.6.0_eslint@4.19.1/node_modules/eslint-config-prettier/bin/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/eslint-config-prettier@3.6.0_eslint@4.19.1/node_modules/eslint-config-prettier/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/eslint-config-prettier@3.6.0_eslint@4.19.1/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/node_modules:/home/workspace/mflix-js/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/mflix-js/node_modules/eslint-config-prettier/bin/cli.js/node_modules:/home/workspace/mflix-js/node_modules/eslint-config-prettier/bin/node_modules:/home/workspace/mflix-js/node_modules/eslint-config-prettier/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../eslint-config-prettier/bin/cli.js" "$@"
else 
  exec node  "$basedir/../eslint-config-prettier/bin/cli.js" "$@"
fi
