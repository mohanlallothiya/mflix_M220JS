#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/mflix-js/node_modules/.pnpm/run-node@1.0.0/node_modules/run-node/run-node/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/run-node@1.0.0/node_modules/run-node/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/run-node@1.0.0/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/node_modules:/home/workspace/mflix-js/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/mflix-js/node_modules/run-node/run-node/node_modules:/home/workspace/mflix-js/node_modules/run-node/node_modules"
if [ -x "$basedir/bash" ]; then
  exec "$basedir/bash"  "$basedir/../run-node/run-node" "$@"
else 
  exec bash  "$basedir/../run-node/run-node" "$@"
fi
