#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/mflix-js/node_modules/.pnpm/staged-git-files@1.1.1/node_modules/staged-git-files/bin/cli.js/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/staged-git-files@1.1.1/node_modules/staged-git-files/bin/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/staged-git-files@1.1.1/node_modules/staged-git-files/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/staged-git-files@1.1.1/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/node_modules:/home/workspace/mflix-js/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/mflix-js/node_modules/staged-git-files/bin/cli.js/node_modules:/home/workspace/mflix-js/node_modules/staged-git-files/bin/node_modules:/home/workspace/mflix-js/node_modules/staged-git-files/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../staged-git-files/bin/cli.js" "$@"
else 
  exec node  "$basedir/../staged-git-files/bin/cli.js" "$@"
fi
