#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/mflix-js/node_modules/.pnpm/he@1.1.1/node_modules/he/bin/he/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/he@1.1.1/node_modules/he/bin/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/he@1.1.1/node_modules/he/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/he@1.1.1/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/node_modules:/home/workspace/mflix-js/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/mflix-js/node_modules/he/bin/he/node_modules:/home/workspace/mflix-js/node_modules/he/bin/node_modules:/home/workspace/mflix-js/node_modules/he/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../he/bin/he" "$@"
else 
  exec node  "$basedir/../he/bin/he" "$@"
fi
