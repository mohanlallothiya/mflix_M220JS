#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/mflix-js/node_modules/.pnpm/tree-kill@1.2.2/node_modules/tree-kill/cli.js/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/tree-kill@1.2.2/node_modules/tree-kill/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/tree-kill@1.2.2/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/node_modules:/home/workspace/mflix-js/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/mflix-js/node_modules/tree-kill/cli.js/node_modules:/home/workspace/mflix-js/node_modules/tree-kill/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tree-kill/cli.js" "$@"
else 
  exec node  "$basedir/../tree-kill/cli.js" "$@"
fi
