#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/mflix-js/node_modules/.pnpm/mocha@5.2.0/node_modules/mocha/bin/mocha/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/mocha@5.2.0/node_modules/mocha/bin/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/mocha@5.2.0/node_modules/mocha/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/mocha@5.2.0/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/node_modules:/home/workspace/mflix-js/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/mflix-js/node_modules/mocha/bin/mocha/node_modules:/home/workspace/mflix-js/node_modules/mocha/bin/node_modules:/home/workspace/mflix-js/node_modules/mocha/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mocha/bin/mocha" "$@"
else 
  exec node  "$basedir/../mocha/bin/mocha" "$@"
fi
