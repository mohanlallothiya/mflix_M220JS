#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/mflix-js/node_modules/.pnpm/jest@25.5.4/node_modules/jest/bin/jest.js/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/jest@25.5.4/node_modules/jest/bin/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/jest@25.5.4/node_modules/jest/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/jest@25.5.4/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/node_modules:/home/workspace/mflix-js/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/mflix-js/node_modules/jest/bin/jest.js/node_modules:/home/workspace/mflix-js/node_modules/jest/bin/node_modules:/home/workspace/mflix-js/node_modules/jest/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jest/bin/jest.js" "$@"
else 
  exec node  "$basedir/../jest/bin/jest.js" "$@"
fi
