#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/mflix-js/node_modules/.pnpm/jsesc@1.3.0/node_modules/jsesc/bin/jsesc/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/jsesc@1.3.0/node_modules/jsesc/bin/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/jsesc@1.3.0/node_modules/jsesc/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/jsesc@1.3.0/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/node_modules:/home/workspace/mflix-js/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/babel-generator@6.26.1/node_modules/babel-generator/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/babel-generator@6.26.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../jsesc@1.3.0/node_modules/jsesc/bin/jsesc" "$@"
else 
  exec node  "$basedir/../../../../../jsesc@1.3.0/node_modules/jsesc/bin/jsesc" "$@"
fi
