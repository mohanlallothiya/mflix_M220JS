#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/mflix-js/node_modules/.pnpm/rc@1.2.8/node_modules/rc/cli.js/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/rc@1.2.8/node_modules/rc/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/rc@1.2.8/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/node_modules:/home/workspace/mflix-js/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/requireg@0.2.2/node_modules/requireg/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/requireg@0.2.2/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../rc@1.2.8/node_modules/rc/cli.js" "$@"
else 
  exec node  "$basedir/../../../../../rc@1.2.8/node_modules/rc/cli.js" "$@"
fi
