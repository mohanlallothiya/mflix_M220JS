#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/mflix-js/node_modules/.pnpm/babylon@6.18.0/node_modules/babylon/bin/babylon.js/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/babylon@6.18.0/node_modules/babylon/bin/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/babylon@6.18.0/node_modules/babylon/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/babylon@6.18.0/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/node_modules:/home/workspace/mflix-js/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/istanbul-lib-instrument@1.10.2/node_modules/istanbul-lib-instrument/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/istanbul-lib-instrument@1.10.2/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../babylon@6.18.0/node_modules/babylon/bin/babylon.js" "$@"
else 
  exec node  "$basedir/../../../../../babylon@6.18.0/node_modules/babylon/bin/babylon.js" "$@"
fi
