#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/mflix-js/node_modules/.pnpm/escodegen@1.14.3/node_modules/escodegen/bin/esgenerate.js/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/escodegen@1.14.3/node_modules/escodegen/bin/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/escodegen@1.14.3/node_modules/escodegen/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/escodegen@1.14.3/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/node_modules:/home/workspace/mflix-js/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/jsdom@11.12.0/node_modules/jsdom/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/jsdom@11.12.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../escodegen@1.14.3/node_modules/escodegen/bin/esgenerate.js" "$@"
else 
  exec node  "$basedir/../../../../../escodegen@1.14.3/node_modules/escodegen/bin/esgenerate.js" "$@"
fi
