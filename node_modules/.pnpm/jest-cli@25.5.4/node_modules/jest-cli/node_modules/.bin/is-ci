#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/mflix-js/node_modules/.pnpm/is-ci@2.0.0/node_modules/is-ci/bin.js/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/is-ci@2.0.0/node_modules/is-ci/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/is-ci@2.0.0/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/node_modules:/home/workspace/mflix-js/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/jest-cli@25.5.4/node_modules/jest-cli/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/jest-cli@25.5.4/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../is-ci@2.0.0/node_modules/is-ci/bin.js" "$@"
else 
  exec node  "$basedir/../../../../../is-ci@2.0.0/node_modules/is-ci/bin.js" "$@"
fi
