#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/mflix-js/node_modules/.pnpm/acorn@7.4.1/node_modules/acorn/bin/acorn/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/acorn@7.4.1/node_modules/acorn/bin/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/acorn@7.4.1/node_modules/acorn/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/acorn@7.4.1/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/node_modules:/home/workspace/mflix-js/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/jsdom@15.2.1/node_modules/jsdom/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/jsdom@15.2.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../acorn@7.4.1/node_modules/acorn/bin/acorn" "$@"
else 
  exec node  "$basedir/../../../../../acorn@7.4.1/node_modules/acorn/bin/acorn" "$@"
fi
