#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/mflix-js/node_modules/.pnpm/npm-path@2.0.4/node_modules/npm-path/bin/npm-path/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/npm-path@2.0.4/node_modules/npm-path/bin/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/npm-path@2.0.4/node_modules/npm-path/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/npm-path@2.0.4/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/node_modules:/home/workspace/mflix-js/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/npm-which@3.0.1/node_modules/npm-which/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/npm-which@3.0.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../npm-path@2.0.4/node_modules/npm-path/bin/npm-path" "$@"
else 
  exec node  "$basedir/../../../../../npm-path@2.0.4/node_modules/npm-path/bin/npm-path" "$@"
fi
