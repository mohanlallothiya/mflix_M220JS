#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/mflix-js/node_modules/.pnpm/mkdirp@0.5.1/node_modules/mkdirp/bin/cmd.js/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/mkdirp@0.5.1/node_modules/mkdirp/bin/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/mkdirp@0.5.1/node_modules/mkdirp/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/mkdirp@0.5.1/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/node_modules:/home/workspace/mflix-js/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/mocha@5.2.0/node_modules/mocha/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/mocha@5.2.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mkdirp@0.5.1/node_modules/mkdirp/bin/cmd.js" "$@"
else 
  exec node  "$basedir/../../../../../mkdirp@0.5.1/node_modules/mkdirp/bin/cmd.js" "$@"
fi
