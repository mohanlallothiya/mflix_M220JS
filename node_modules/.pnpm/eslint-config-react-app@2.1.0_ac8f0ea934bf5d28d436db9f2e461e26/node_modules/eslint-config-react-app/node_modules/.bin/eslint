#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/mflix-js/node_modules/.pnpm/eslint@4.19.1/node_modules/eslint/bin/eslint.js/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/eslint@4.19.1/node_modules/eslint/bin/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/eslint@4.19.1/node_modules/eslint/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/eslint@4.19.1/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/node_modules:/home/workspace/mflix-js/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/eslint-config-react-app@2.1.0_ac8f0ea934bf5d28d436db9f2e461e26/node_modules/eslint-config-react-app/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/eslint-config-react-app@2.1.0_ac8f0ea934bf5d28d436db9f2e461e26/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../eslint@4.19.1/node_modules/eslint/bin/eslint.js" "$@"
else 
  exec node  "$basedir/../../../../../eslint@4.19.1/node_modules/eslint/bin/eslint.js" "$@"
fi
