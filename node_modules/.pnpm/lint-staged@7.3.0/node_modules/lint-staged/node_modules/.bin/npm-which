#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/mflix-js/node_modules/.pnpm/npm-which@3.0.1/node_modules/npm-which/bin/npm-which.js/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/npm-which@3.0.1/node_modules/npm-which/bin/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/npm-which@3.0.1/node_modules/npm-which/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/npm-which@3.0.1/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/node_modules:/home/workspace/mflix-js/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/lint-staged@7.3.0/node_modules/lint-staged/node_modules:/home/workspace/mflix-js/node_modules/.pnpm/lint-staged@7.3.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../npm-which@3.0.1/node_modules/npm-which/bin/npm-which.js" "$@"
else 
  exec node  "$basedir/../../../../../npm-which@3.0.1/node_modules/npm-which/bin/npm-which.js" "$@"
fi
